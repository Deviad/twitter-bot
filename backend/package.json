  "license": "MIT",
  "scripts": {
    "watch": "NODE_ENV=development nodemon -e ts -x ts-node src/bootstrap.ts",
    "watch:debug": "NODE_ENV=development node --inspect=0.0.0.0:9229 --require ts-node/register src/bootstrap.ts",
    "start:dev": "ts-node src/bootstrap.ts",
    "start": "NODE_ENV=production ts-node src/bootstrap.ts",
    "start:staging": "NODE_ENV=staging ts-node src/bootstrap.ts",
    "test": "jest --detectOpenHandles --colors"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "1.19.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.4",
    "express": "4.17.1",
    "helmet": "4.4.1",
    "inversify": "5.0.5",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.3.2",
    "inversify-logger-middleware": "3.1.0",
    "lodash": "^4.17.21",
    "mongodb": "3.6.4",
    "morgan": "1.10.0",
    "reflect-metadata": "0.1.13",
    "twitter-lite": "^1.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.33",
    "@types/body-parser": "1.19.0",
    "@types/chai": "4.2.15",
    "@types/express": "4.17.11",
    "@types/helmet": "0.0.48",
    "@types/mocha": "8.2.1",
    "@types/mongodb": "3.6.9",
    "@types/morgan": "1.9.2",
    "@types/node-schedule": "^1.3.1",
    "node-schedule": "^2.0.0",
    "nodemon": "^2.0.7",
    "ts-helpers": "^1.1.2",
    "ts-node": "9.1.1",
    "tslib": "^2.1.0",
    "tslint": "^5.20.1",
    "typescript": "^4"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.ts"
    ]
  }
